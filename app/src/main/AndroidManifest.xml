<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.registrationsystem">

    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="Registration System"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ListViewShower" ></activity>
        <activity android:name=".AddStudent" />
        <activity android:name=".ShowStudents" />
        <activity android:name=".UpdateStudent" />
        <activity android:name=".DeleteStudent" />
        <activity android:name=".MainActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <meta-data
        android:name="preloaded_fonts"
        android:resource="@array/preloaded_fonts" />
</application>

    </manifest>